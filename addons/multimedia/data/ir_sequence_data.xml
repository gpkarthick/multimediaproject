<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_multimedia" model="ir.sequence">
            <field name="name">Multi Media</field>
            <field name="code">multimedia.eb.bill</field>
            <field name="prefix">EB/</field>
            <field name="padding">5</field>
            <field name="suffix">/%(y)s</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_multimedia_auto_number" model="ir.sequence">
            <field name="name">Multi Media Auto no</field>
            <field name="code">multimedia.eb.bill.auto.no</field>
            <field name="prefix">PGNINB</field>
            <field name="padding">7</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_multimedia_purchase_bill" model="ir.sequence">
            <field name="name">Purchase Bill</field>
            <field name="code">purchase.bills</field>
            <field name="prefix">PO</field>
            <field name="padding">5</field>
            <field name="suffix">/%(y)s</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_multimedia" model="ir.sequence">
            <field name="name">Multi Media</field>
            <field name="code">multimedia.eb.bill</field>
            <field name="prefix">EB/</field>
            <field name="padding">5</field>
            <field name="suffix">/%(y)s</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--        Account Verification-->
        <record id="seq_account_verification_ref" model="ir.sequence">
            <field name="name">Account Verification Ref</field>
            <field name="code">account.verification.ref</field>
            <field name="prefix">23322215</field>
            <field name="padding">4</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_account_verification_order_id" model="ir.sequence">
            <field name="name">Account Verification Order ID</field>
            <field name="code">account.verification.order</field>
            <field name="prefix">GP34227</field>
            <field name="padding">4</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_multimedia_purchase_bill" model="ir.sequence">
            <field name="name">Purchase Bill</field>
            <field name="code">purchase.bills</field>
            <field name="prefix">PO</field>
            <field name="padding">5</field>
            <field name="suffix">/%(y)s</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_pan_form" model="ir.sequence">
            <field name="name">Pan Form</field>
            <field name="code">pan.form</field>
            <field name="prefix">PAN-%(month)s-</field>
            <field name="padding">4</field>
            <field name="suffix">/%(y)s</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_rationcard_form" model="ir.sequence">
            <field name="name">Pan Form</field>
            <field name="code">pan.form.ration</field>
            <field name="prefix">RC-%(month)s-</field>
            <field name="padding">4</field>
            <field name="suffix">/%(y)s</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_acknowledgement_slip" model="ir.sequence">
            <field name="name">Acknowledgement Slip</field>
            <field name="code">acknowledgement.slip</field>
            <field name="prefix">AS-%(month)s-</field>
            <field name="padding">5</field>
            <field name="suffix">/%(y)s</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
