<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="multimedia_farmer_insurance_tree_view" model="ir.ui.view">
            <field name="name">farmer.insurance.tree</field>
            <field name="model">farmer.insurance</field>
            <field name="arch" type="xml">
                <tree string="Insurance" decoration-danger="state in ('paid')" decoration-success="state in ('draft')">
                    <field name="name"/>
                    <field name="original_receipt_no"/>
                    <field name="insurance_date"/>
                    <field name="farmer_name"/>
                    <field name="village_id"/>
                    <field name="create_date"/>
                    <field name="total_area_insured" string="Area Insured"/>
                    <field name="total_sum_insured" string="Sum Insured"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>

        <record id="multimedia_farmer_insurance_form_view" model="ir.ui.view">
            <field name="name">farmer.insurance.form</field>
            <field name="model">farmer.insurance</field>
            <field name="arch" type="xml">
                <form string="Insurance Form">
                    <header>
                        <button name="insurance_bill_print" string="Print" type="object" class="oe_highlight"
                                attrs="{'invisible': [('state', '=', 'draft')]}"/>
                        <button name="insurance_tamil_bill_print" string="Tamil Print" type="object"
                                class="oe_highlight" attrs="{'invisible': [('state', '=', 'draft')]}"/>
                        <button name="action_reset" type="object" string="Reset" class="oe_highlight"
                                attrs="{'invisible': [('state', 'in', 'draft')]}"/>
                        <button name="action_pending" type="object" string="Pending" class="oe_highlight"
                                attrs="{'invisible': [('state', 'in', ('paid','pending'))]}"/>
                        <button name="action_complete" type="object" string="Done Original Bill" class="oe_highlight"
                                attrs="{'invisible': [('state', 'in', ('paid'))]}"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,paid"/>
                    </header>
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <field name="seq_no" readonly="1"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <div style="color:#873260;font-size:12px;text-align:center">
                                    <h1>
                                        <b>
                                            English
                                        </b>
                                    </h1>
                                </div>
                            </group>
                            <group>
                                <div style="color:#873260;font-size:12px;text-align:center">
                                    <h1>
                                        <b>
                                            Tamil
                                        </b>
                                    </h1>
                                </div>
                            </group>
                        </group>
                        <group>
                            <group>
                                <field name="name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="original_receipt_no" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="insurance_date" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="mobile_no" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>
                            <group>
                                <field name="name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="original_receipt_no" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="insurance_date" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="mobile_no" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>
                        </group>
                        <group>
                            <group>
                                <field name="state_id" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="scheme_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="year_val" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="application_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="season_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="created_by" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="create_at" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>
                            <group>
                                <field name="state_id_tamil" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="scheme_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="year_val" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="application_type_tamil" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="season_name_tamil" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="created_by" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="create_at" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>
                        </group>
                        <group>
                            <group>
                                <field name="farmer_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="relative_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="relative_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="mobile_no" invisible="1"/>
                                <field name="farmer_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="gender_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>
                            <group>
                                <field name="farmer_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="relative_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="relative_tamil_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="farmer_tamil_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="gender_tamil_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>
                        </group>
                        <group>
                            <group>
                                <field name="ifsc_code" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="bank_account_no" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="bank_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="branch_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="account_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>
                            <group>
                                <field name="ifsc_code" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="bank_account_no" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="bank_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="branch_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="account_tamil_type" attrs="{'readonly': [('state', '=', 'paid')]}"/>

                            </group>
                        </group>

                        <group>
                            <group>
                                <field name="village_id" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="firka_id" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="block_id" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="subdistrict_id" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="district_id" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="state2_id" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>
                            <group>
                                <field name="village_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="firka_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="block_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="subdistrict_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="district_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                                <field name="state_tamil_name" attrs="{'readonly': [('state', '=', 'paid')]}"/>
                            </group>

                        </group>

                        <group>
                            <group>
                                <field name="total_area_insured"/>
                                <field name="total_sum_insured"/>
                            </group>
                            <group>
                                <field name="total_premium_paid"/>
                            </group>
                        </group>
                        <group>
                            <group>
                                <field name="qr_code" widget="image" class="oe_avatar" nolabel="1"/>
                            </group>
                            <group>
                                <field name="create_uid"/>
                                <field name="write_uid"/>
                                <field name="create_date"/>
                                <field name="write_date"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Crop Details">
                                <field name="crop_line_ids" attrs="{'readonly':[('state','in',['done'])]}">
                                    <tree>
                                        <field name="survey_no"/>
                                        <field name="khasra_no"/>
                                        <field name="crop_data"/>
                                        <field name="area_insured" sum="area_insured"/>
                                        <field name="farmer_share" sum="farmer_share"/>
                                        <field name="gov_share" sum="gov_share"/>
                                        <field name="sum_insured" sum="sum_insured"/>
                                    </tree>
                                    <form string="Drop Details">
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="survey_no"/>
                                                    <field name="khasra_no"/>
                                                    <field name="crop_data"/>
                                                    <field name="area_insured" sum="area_insured"/>
                                                    <field name="farmer_share" sum="farmer_share"/>
                                                    <field name="gov_share" sum="gov_share"/>
                                                    <field name="sum_insured" sum="sum_insured"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_multimedia_farmer_insurance_search" model="ir.ui.view">
            <field name="name">Farmer Insurance Search</field>
            <field name="model">farmer.insurance</field>
            <field name="arch" type="xml">
                <search string="Insurance">
                    <field name="name" string="Insurance No" filter_domain="[('name','ilike',self)]"/>
                    <field name="insurance_date" string="Insurance Date"
                           filter_domain="[('insurance_date','ilike',self)]"/>
                    <field name="state" string="State" filter_domain="[('state','ilike',self)]"/>
                    <separator/>
                    <filter string="Paid" name="paid" domain="[('state','=','paid')]"/>
                    <filter string="Draft" name="draft" domain="[('state','=','draft')]"/>
                    <group expand="1" string="Group By">
                        <filter string="Month" name="insurance_date" context="{'group_by':'insurance_date:month'}"
                                help="Insurance Date"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="action_farmer_insurance_view" model="ir.actions.act_window">
            <field name="name">Insurance</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">farmer.insurance</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
            <field name="domain">[]</field>
            <field name="search_view_id" ref="view_multimedia_farmer_insurance_search"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a Farmer Insurance Here
                </p>
                <p>
                    We can track the Farmer Insurance here
                </p>
            </field>
        </record>


