<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
	<template id="multi_base_layout" inherit_id="web.layout" name="layout" priority="17">
		<xpath expr="//head//link" position="replace">
			<link rel="shortcut icon" href="/multimedia/static/src/img/favicon.ico" type="image/x-icon"/>
		</xpath>
		<xpath expr="//head//title" position="replace">
			<title t-esc="title or 'Prime'"/>
		</xpath>
	</template>
	
	<template id="assets_login" inherit_id="web.assets_frontend">
		<xpath expr="." position="inside">
			<link rel="stylesheet" href="/multimedia/static/src/css/web_login_style.css"/>
		</xpath>
	</template>
	
	
</odoo>
